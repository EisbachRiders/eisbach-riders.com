{
  "name": "eisbach-riders",
  "description": "Eisbach Riders - Surf Accessories",
  "version": "1.0.0",
  "author": "Alyssa Butler <ca.butler10@gmail.com>",
  "keywords": [
    "eisbach, surf, surf accessories"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabutler10/website_EisbachRidersGatsbyGatsby.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "jest",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "build:ci": "node_modules/.bin/gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.118",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-feed": "^2.0.13",
    "gatsby-plugin-google-analytics": "^2.0.13",
    "gatsby-plugin-mailchimp": "^4.0.0",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-offline": "^2.0.23",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-remark-copy-linked-files": "^2.0.9",
    "gatsby-remark-images": "^3.0.3",
    "gatsby-remark-responsive-iframe": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-remark": "^2.2.4",
    "gatsby-transformer-sharp": "^2.1.13",
    "i18next": "^14.1.1",
    "i18next-browser-languagedetector": "^2.2.4",
    "jss": "^9.8.7",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-i18next": "^10.0.2",
    "react-jss": "^8.6.1",
    "react-swipeable-views": "^0.13.1",
    "typeface-contrail-one": "^0.0.54",
    "typeface-open-sans": "^0.0.54"
  },
  "devDependencies": {
    "babel-jest": "^24.1.0",
    "babel-preset-gatsby": "^0.1.7",
    "eslint": "^5.13.0",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-dom": "^3.0.2",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.1",
    "react-testing-library": "^5.5.4"
  }
}
