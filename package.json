{
  "name": "eisbach-riders",
  "description": "Eisbach Riders - Surf Accessories",
  "version": "1.0.0",
  "author": "Alyssa Butler <ca.butler10@gmail.com>",
  "keywords": [
    "eisbach, surf, surf accessories"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabutler10/website_EisbachRidersGatsbyGatsby.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "jest --coverage -u",
    "test-coveralls": "jest --coverage -u && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "build:ci": "node_modules/.bin/gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.0",
    "@mdx-js/mdx": "^1.0.27",
    "@mdx-js/react": "^1.0.27",
    "change-case": "^3.1.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.13.20",
    "gatsby-image": "^2.2.6",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-feed": "^2.3.4",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-jss": "^2.1.3",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-material-ui": "^2.1.4",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.7",
    "gatsby-plugin-sitemap": "^2.2.3",
    "gatsby-remark-copy-linked-files": "^2.1.3",
    "gatsby-remark-images": "^3.1.6",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-source-graphql": "^2.1.2",
    "gatsby-source-wordpress": "^3.1.8",
    "gatsby-transformer-remark": "^2.6.5",
    "gatsby-transformer-sharp": "^2.2.3",
    "i18next": "^17.0.6",
    "i18next-browser-languagedetector": "^3.0.1",
    "jss": "^9.8.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-i18next": "^10.11.4",
    "react-jss": "^8.6.1",
    "react-redux": "^7.1.0",
    "react-swipeable-views": "^0.13.3",
    "recharts": "^1.6.2",
    "redux": "^4.0.4",
    "typeface-open-sans": "^0.0.54",
    "typeface-raleway": "^0.0.54"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.5",
    "babel-jest": "^24.8.0",
    "babel-preset-gatsby": "^0.2.5",
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-axe": "^3.1.1",
    "jest-dom": "^4.0.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6"
  }
}
