{
  "name": "eisbach-riders",
  "description": "Eisbach Riders - Surf Accessories",
  "version": "1.0.0",
  "author": "Alyssa Butler <ca.butler10@gmail.com>",
  "keywords": [
    "eisbach, surf, surf accessories"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabutler10/website_EisbachRidersGatsbyGatsby.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "jest --coverage -u",
    "test-coveralls": "jest --coverage -u && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "build:ci": "node_modules/.bin/gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@mdx-js/mdx": "^1.5.3",
    "@mdx-js/react": "^1.5.3",
    "clsx": "^1.0.4",
    "gatsby": "^2.18.17",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-facebook-pixel": "^1.0.3",
    "gatsby-plugin-feed": "^2.3.25",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-jss": "^2.1.17",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-mdx": "^1.0.64",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-images": "^3.1.39",
    "gatsby-remark-responsive-iframe": "^2.2.30",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-source-graphql": "^2.1.29",
    "gatsby-source-wordpress": "^3.1.54",
    "gatsby-transformer-remark": "^2.6.45",
    "gatsby-transformer-sharp": "^2.3.9",
    "i18next": "^19.0.2",
    "i18next-browser-languagedetector": "^4.0.1",
    "jss": "^10.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.2.7",
    "react-jss": "^10.0.3",
    "react-player": "^1.14.2",
    "react-redux": "^7.1.3",
    "react-swipeable-views": "^0.13.3",
    "recharts": "1.8.5",
    "redux": "^4.0.5",
    "typeface-open-sans": "^0.0.75",
    "typeface-raleway": "^0.0.75",
    "victory": "^34.0.0",
    "wp-graphql": "^1.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.26",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  }
}
