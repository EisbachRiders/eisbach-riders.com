{
  "name": "eisbach-riders",
  "description": "Eisbach Riders - Surf Accessories",
  "version": "1.0.0",
  "author": "Alyssa Butler <ca.butler10@gmail.com>",
  "keywords": [
    "eisbach, surf, surf accessories"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabutler10/website_EisbachRidersGatsbyGatsby.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "jest --coverage -u",
    "test-coveralls": "jest --coverage -u && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "build:ci": "node_modules/.bin/gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "clsx": "^1.1.0",
    "gatsby": "^2.20.12",
    "gatsby-background-image": "^1.0.1",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-facebook-pixel": "^1.0.3",
    "gatsby-plugin-feed": "^2.4.1",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-jss": "^2.2.1",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-mdx": "^1.1.4",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-source-contentful": "^2.2.6",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-source-graphql": "^2.3.1",
    "gatsby-source-instagram": "^0.7.0",
    "gatsby-source-wordpress": "^3.2.3",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^4.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.2.0",
    "react-swipeable-views": "^0.13.9",
    "redux": "^4.0.5",
    "typeface-open-sans": "^0.0.75",
    "typeface-raleway": "^0.0.75",
    "victory": "^34.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "babel-jest": "^25.2.6",
    "babel-preset-gatsby": "^0.3.1",
    "contentful-import": "^7.7.3",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "jest-axe": "^3.4.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.12.0"
  }
}
